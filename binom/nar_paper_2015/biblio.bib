  Timestamp                = {2015.01.28},
  Url                      = {http://dx.doi.org/10.1093/bioinformatics/btt285}
}

@Article{Nishida2014,
  Title                    = {KEGGscape: a Cytoscape app for pathway data integration.},
  Author                   = {Nishida, Kozo and Ono, Keiichiro and Kanaya, Shigehiko and Takahashi, Koichi},
  Journal                  = {F1000Res},
  Year                     = {2014},
  Pages                    = {144},
  Volume                   = {3},

  Abstract                 = {In this paper, we present KEGGscape a pathway data integration and visualization app for Cytoscape ( http://apps.cytoscape.org/apps/keggscape). KEGG is a comprehensive public biological database that contains large collection of human curated pathways. KEGGscape utilizes the database to reproduce the corresponding hand-drawn pathway diagrams with as much detail as possible in Cytoscape. Further, it allows users to import pathway data sets to visualize biologist-friendly diagrams using the Cytoscape core visualization function (Visual Style) and the ability to perform pathway analysis with a variety of Cytoscape apps. From the analyzed data, users can create complex and interactive visualizations which cannot be done in the KEGG PATHWAY web application. Experimental data with Affymetrix E. coli chips are used as an example to demonstrate how users can integrate pathways, annotations, and experimental data sets to create complex visualizations that clarify biological systems using KEGGscape and other Cytoscape apps.},
  Doi                      = {10.12688/f1000research.4524.1},
  Institution              = {Laboratory for Biochemical Simulation, RIKEN Quantitative Biology Center, Osaka, 565-0874, Japan.},
  Language                 = {eng},
  Medline-pst              = {epublish},
  Owner                    = {zinovyev},
  Pmid                     = {25177485},
  Timestamp                = {2015.01.28},
  Url                      = {http://dx.doi.org/10.12688/f1000research.4524.1}
}

@Article{vanIersel2008,
  Title                    = {Presenting and exploring biological pathways with PathVisio.},
  Author                   = {{van Iersel}, Martijn P. and Kelder, Thomas and Pico, Alexander R. and Hanspers, Kristina and Coort, Susan and Conklin, Bruce R. and Evelo, Chris},
  Journal                  = {BMC Bioinformatics},
  Year                     = {2008},
  Pages                    = {399},
  Volume                   = {9},

  __markedentry            = {[zinovyev:6]},
  Abstract                 = {Biological pathways are a useful abstraction of biological concepts, and software tools to deal with pathway diagrams can help biological research. PathVisio is a new visualization tool for biological pathways that mimics the popular GenMAPP tool with a completely new Java implementation that allows better integration with other open source projects. The GenMAPP MAPP file format is replaced by GPML, a new XML file format that provides seamless exchange of graphical pathway information among multiple programs.PathVisio can be combined with other bioinformatics tools to open up three possible uses: visual compilation of biological knowledge, interpretation of high-throughput expression datasets, and computational augmentation of pathways with interaction information. PathVisio is open source software and available at http://www.pathvisio.org.PathVisio is a graphical editor for biological pathways, with flexibility and ease of use as primary goals.},
  Doi                      = {10.1186/1471-2105-9-399},
  Institution              = {Department of Bioinformatics, BiGCaT Maastricht University, Universiteitssingel 40, 6229 ER Maastricht, the Netherlands. martijn.vaniersel@bigcat.unimaas.nl},
  Keywords                 = {Algorithms; Computer Graphics; Computer Simulation; Models, Biological; Proteome, metabolism; Signal Transduction, physiology; Software; User-Computer Interface},
  Language                 = {eng},
  Medline-pst              = {epublish},
  Owner                    = {zinovyev},
  Pii                      = {1471-2105-9-399},
  Pmid                     = {18817533},
  Timestamp                = {2015.01.28},
