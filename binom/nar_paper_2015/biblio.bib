@Article{Gehlenborg2010,
  Title                    = {Visualization of omics data for systems biology.},
  Author                   = {Gehlenborg, Nils and O'Donoghue, Se{\'{a}}n I. and Baliga, Nitin S. and Goesmann, Alexander and Hibbs, Matthew A. and Kitano, Hiroaki and Kohlbacher, Oliver and Neuweger, Heiko and Schneider, Reinhard and Tenenbaum, Dan and Gavin, Anne-Claude},
